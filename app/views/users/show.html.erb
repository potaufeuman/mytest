<% provide(:title, @user.name) %>
<div class = "row">
  <%= render "side_menu", user: @user %>
  <div class="col-md-8 white">
    <% if @user.tests.any? %>
      <h3>Tests (<%= @user.tests.count %>)</h3>
      <ol class="microposts">
        <%= render @tests, user: @user  %>
      </ol>
      <%= will_paginate @tests %>
    <% end %>
  </div>
</div>
