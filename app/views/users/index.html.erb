<% provide(:title, 'All users') %>
<div class="white_belt"><h1><%= yield(:title) %></h1></div>
<div class = "row">
  <%= render "side_menu", user: @user %>
  <div class = "col-md-7 white_block">
    <%= will_paginate @users %>
    <ul class="users">
      <% @users.each do |user| %>
        <%= render user %>
      <% end %>
    </ul>
    <%= will_paginate %>
  </div>
</div>