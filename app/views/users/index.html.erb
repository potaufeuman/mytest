<% provide(:title, 'All users') %>
<h1><%= yield(:title) %></h1>
<div class = "row">
  <%= render "side_menu", user: @user %>
  <div class = "col-md-8">
    <%= will_paginate @users %>
    <ul class="users">
      <% @users.each do |user| %>
        <%= render user %>
      <% end %>
      <!--= render @users %>-->
    </ul>
    <%= will_paginate %>
  </div>
</div>