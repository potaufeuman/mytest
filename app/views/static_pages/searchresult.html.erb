  <div class="col-md-7 white_block">
    <h3>Search Result (<%= @search_tests.count %>)</h3>
    <ol class="microposts">
      <%= render @search_tests  %>
    </ol>
    <%= will_paginate @search_tests %>
  </div>
</div>