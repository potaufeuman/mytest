<% provide(:title, @user.name) %>
<div class="row">
  <%= render "users/side_menu", user: @user %>
  <div class="col-md-8">
    <% if @user.tests.any? %>
      <h3>Microposts (<%= @user.tests.count %>)</h3>
      <ol class="microposts">
        <%= render @tests %>
      </ol>
      <%= will_paginate @tests %>
    <% end %>
  </div>
</div>